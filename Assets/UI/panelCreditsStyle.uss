.panelCreditsStyle {
    justify-content: center;
    align-items: center;
    background-color: rgba(255, 255, 255, 0.58);
    padding-top: 0;
    padding-right: 0;
}

.containerCreditsStyle {
    width: 75%;
    height: 80%;
    max-height: 80%;
    background-color: rgba(182, 182, 182, 0.48);
    align-items: stretch;
    justify-content: space-between;
    align-self: auto;
    border-top-left-radius: 40px;
    border-top-right-radius: 40px;
    border-bottom-right-radius: 40px;
    border-bottom-left-radius: 40px;
    padding-right: 50px;
    padding-left: 50px;
}

.regresarBtnStyle {
    width: 400px;
    height: 100px;
    margin-top: 50px;
    margin-left: 0;
    font-size: 50px;
    -unity-text-outline-color: rgba(217, 0, 0, 0);
    color: rgb(217, 0, 0);
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    background-color: rgb(226, 226, 226);
    justify-content: flex-end;
    align-items: stretch;
    align-self: flex-end;
}
